    "def clock():\n",
    "    # YOUR CODE HERE\n",
    "    n = int(input())\n",
    "    n = n % (3600 * 24)\n",
    "    h = n // 3600\n",
    "    m = n % 3600 // 60\n",
    "    s = n % 60\n",
    "    print(h // 10, h % 10, ':', m // 10, m % 10, ':', s // 10,s % 10)\n",
    "clock()"
